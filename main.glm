#ifmissing "123.glm"
#model get IEEE/123
#endif
#include "123.glm"

// Task 1 - Add a diesel generation to Node 8 (1 cyl, 2 stroke, speed 360 rpm, torque 680 N)
module generators;
object diesel_dg 
{
    parent node_8;
    Gen_mode CONSTANTPQ;
    Gen_status ONLINE;
    Gen_type SYNCHRONOUS;
    pf 1.0;
    phases 1;
    cylinders 1;
    stroke 2;
    speed 360 1/min;
    torque 680;
    power_out_A load_9.constant_power_A;
    
}

// Task 2 - Add 4kW 100 sf solar panel on load 10 (tilt 45, efficiency 0.135, south facing, fixed) 
#ifmissing "CA-Chino_Airport.glm" //https://nsrdb.nrel.gov/data-viewer
#weather get CA-Chino_Airport
#endif
#input "CA-Chino_Airport.tmy3"
clock
{
    timezone America/Los_Angeles;
    starttime "2020-07-01 12:00:00 PDT";
    stoptime "2020-07-01 12:00:00 PDT";
}
object meter
{
    name meter_10;
    parent load_10;
    phases AN;
    nominal_voltage 2401.7711;
    object inverter 
    {
        name inv_10; // DC = AC/sqrt(2)
        phases AN;
        rated_power 17107 kVA;
        object solar 
        {
            name solar_10;
            parent inv_10;
            rated_power 24193.5 kVA; //6.0-6.5 (6.25 avg) solar irradiance * 92.9 m^2/ft^2 * 1000 ft^2/24 hr, power factor = 1?
            //rating = 1000 kw/m^2 * areas * efficiency
            area 1000 sf;
            tilt_angle 45.0 deg;
            efficiency 13.5%;
            orientation_azimuth 180;
        };
    };
}

// Task 3 - Wind turbine on load 28
object meter {
    name meter_28;
    parent load_28;
    phases ABCN;
    nominal_voltage 2401.7711;
    object windturb_dg 
    {
        parent meter_28;
        phases ABCN;
        Gen_status ONLINE;
        Gen_type INDUCTION;
        Gen_mode CONSTANTP;
        Turbine_Model GENERIC_IND_SMALL;
        Rated_VA 100 kVA;
    };
}

#output "IEEE-123-voltage-profile.png" -t profile -l 10
